[user]
	name = fenekku
	email = fenekku@fenekku.com

[alias]
  co = checkout
  ci = commit
  st = status -s
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  sub = reset HEAD --

[color]
	ui = true

[core]
	editor = subl -n -w
[push]
	default = current
[credential]
	helper = cache --timeout 3600
